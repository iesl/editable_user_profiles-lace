{
  "model_name": "upnfconsent",
  "train_suffix": "tfidfcsrr",
  "consent-base-pt-layer": "allenai/aspire-contextualsentence-multim-compsci",
  "kp-base-pt-layer": "Sheshera/lace-kp-encoder-compsci",
  "score_aggregation": "l2wasserstein",
  "ict_train_frac": 0.5,
  "ict_lossprop": 0.1,
  "geoml_blur": 0.05,
  "geoml_scaling": 0.9,
  "sent_sm_temp": 5000,
  "mag_prune_query": true,
  "mag_prune_fraction": 0.8,
  "mag_prune_cand": true,
  "mag_prune_fraction_cand": 0.5,
  "normalize_points": false,
  "barycenter_projection": true,
  "cl_geoml_blur": 0.05,
  "cl_geoml_scaling": 0.9,
  "cl_sent_sm_temp": 5000,
  "kps_per_abs": 20,
  "num_epochs": 4,
  "batch_size": 24,
  "update_rule": "adam",
  "learning_rate": 2e-5,
  "num_warmup_steps": 500,
  "decay_lr_every": 1,
  "lr_decay_method": "warmuplin",
  "decay_lr_by": 0.95,
  "es_check_every": 3000,
  "sc_fine_tune": true,
  "kp_fine_tune": true,
  "loss_margin": 0.1,
  "only_init": true
}